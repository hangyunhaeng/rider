	function 선택된전체금액계산(){
		var sum = 0;

		for(var i = 0 ; i < $(":checked").parent().find('input[name=price]').length ; i++ ){
			sum += Number($($(":checked").parent().find('input[name=price]')[i]).val());
		}

 		$('#선택금액').text(Number(sum).toLocaleString()+"원");
	}
	function 천체체크확인(){
		if($('input[type=checkbox]').length ==  $(":checked").length){
			//전체 선택됨
			$($('input[type=checkbox]')[0]).prop("checked", true);
		} else {
			$($('input[type=checkbox]')[0]).prop("checked", false);
		}
